{
  "expo": {
    "owner": "mynuu",
    "name": "MynuuAppReg",
    "slug": "Mynuu-Production",
    "version": "3.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.4"
          },
          "android": {
            "deploymentTarget": "10.0.0"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bundle.mynuu",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes for inventory management."
      }
    },
    "android": {
      "package": "com.bundle.mynuu",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e19bb044-9652-4d1a-9535-6e1e9dd55014"
      }
    }
  }
}
